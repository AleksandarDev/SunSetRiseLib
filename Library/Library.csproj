<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <RootNamespace>KoenZomers.Tools.SunSetRiseLib</RootNamespace>
    <AssemblyName>KoenZomers.Tools.SunSetRiseLib</AssemblyName>
    <TargetFrameworks>netstandard2.0;netcoreapp2.0;net452;net472</TargetFrameworks>
    <SignAssembly>true</SignAssembly>
    <AssemblyOriginatorKeyFile>Key.snk</AssemblyOriginatorKeyFile>
    <AssemblyVersion>1.2.0.3</AssemblyVersion>
    <FileVersion>1.2.0.3</FileVersion>
    <Version>1.2.0.3</Version>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Authors>Koen Zomers</Authors>
    <Company>Koen Zomers</Company>
    <Description>Library in .NET Standard 2.0, .NET Framework 4.5.2, .NET Framework 4.7.2 and .NET Core 2.0 to calculate the sunrise and sunset at any given date anywhere in the world</Description>
    <PackageProjectUrl>https://github.com/KoenZomers/SunSetRiseLib</PackageProjectUrl>
    <PackageReleaseNotes>Recreated package to target .NET Standard 2.0, .NET Framework 4.5.2, .NET Framework 4.7.2 and .NET Core 2.0</PackageReleaseNotes>
    <PackageId>KoenZomers.SunSetRise</PackageId>
    <Product>KoenZomers.SunSetRise</Product>
  </PropertyGroup>
  <PropertyGroup Condition=" '$(TargetFramework)' == 'net20'">
    <DefineConstants>NET20</DefineConstants>
  </PropertyGroup>
  <PropertyGroup Condition=" '$(Configuration)|$(Platform)' == 'Debug|AnyCPU' ">
    <DefineConstants>DEBUG;TRACE</DefineConstants>
  </PropertyGroup>
  <PropertyGroup Condition="'$(Configuration)|$(TargetFramework)|$(Platform)'=='Release|netstandard1.1|AnyCPU'">
    <DocumentationFile>C:\Code\GitHub\SunSetRiseLib\SunSetRiseLib\Library\KoenZomers.Tools.SunSetRiseLib.xml</DocumentationFile>
  </PropertyGroup>
</Project>